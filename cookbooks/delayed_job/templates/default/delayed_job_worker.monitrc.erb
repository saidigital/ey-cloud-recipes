check process <%= @worker_name %>
  with pidfile /data/<%= @app_name %>/shared/pids/<%= @worker_name %>.pid
  start program = "/data/<%= @app_name %>/current/script/delayed_job -e <%= @framework_env %> start" as uid <%= @user %>
  stop program = "/data/<%= @app_name %>/current/script/delayed_job -e <%= @framework_env %> start" as uid <%= @user %>
  group <%= @app_name %>_jobs
